{
  "name": "ninja-base",
  "version": "0.2.35",
  "description": "Base level functions, classes, interfaces for Ninja",
  "author": "Peer-to-peer Privacy Systems Research LLC",
  "license": "PROPRIETARY",
  "bugs": {
    "url": "https://github.com/p2ppsr/ninja-base/issues"
  },
  "homepage": "https://github.com/p2ppsr/ninja-base#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/p2ppsr/ninja-base.git"
  },
  "keywords": [
    "BSV",
    "Bitcoin"
  ],
  "main": "./out/src/index.js",
  "types": "./out/src/index.d.ts",
  "files": [
    "out",
    "src"
  ],
  "scripts": {
    "clean": "rd /s /q out>nul 2>&1|echo.>nul",
    "build": "tsc --build",
    "build:readme": "npx ts2md",
    "prepublish": "npm run build && npm run build:readme",
    "lint": "ts-standard src/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "tolocal": "npm i ../cwi-base --silent && npm run build",
    "tonpm": "npm run tonone && npm i cwi-base --silent && npm run build",
    "tonone": "npm remove cwi-base --silent"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.1",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "@types/node-fetch": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "jest": "^29.4.3",
    "mock-knex": "^0.4.11",
    "nodemon": "^2.0.4",
    "standard": "^16.0.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "ts-standard": "^12.0.2",
    "ts2md": "^0.1.12",
    "typescript": "^4.9.5",
    "utxoninja": "^0.2.49"
  },
  "dependencies": {
    "authrite-js": "^0.2.16",
    "authrite-utils": "^0.1.16",
    "babbage-bsv": "^0.1.1",
    "cwi-base": "^0.3.23",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.3",
    "node-fetch": "^2.6.12",
    "sendover": "^1.1.9"
  }
}
